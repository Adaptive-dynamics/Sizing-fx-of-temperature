@article{hilborn_general_1987,
	title = {A General Model for Simulation of Stock and Fleet Dynamics in Spatially Heterogeneous Fisheries},
	volume = {44},
	issn = {0706-652X},
	url = {http://www.nrcresearchpress.com/doi/abs/10.1139/f87-163},
	doi = {10.1139/f87-163},
	abstract = {A simple simulation model of stock and fleet dynamics is described that allows rapid exploration of the dynamics of fisheries in a spatially heterogeneous environment. The model simulates how fishermen allocate their search effort among spatial areas in relation to differential catch rates, prices, or area-specific desirabilities. The model runs in a few seconds per simulated year on an {IBM} microcomputer in interpreted {BASIC}, although both age structure and space are explicitly represented. The importance of exploring dynamic behavior of fisheries systems is discussed., On décrit un simple modèle de simulation de la dynamique stock–flottille qui permet l'étude rapide de la dynamique des pêches dans un environnement hétérogène dans l'espace. Le modèle simule la manière dont les pêcheurs répartissent leur effort de recherche parmi des zones spatiales par rapport au taux de capture différentiel, aux prix et aux avantages particuliers d'une région. Le modèle, exécuté en quelques secondes par année simulée, passe en langage {BASIC} décodé sur un micro-ordinateur {IBM}; la structure selon l'âge et l'espace est malgré tout représentée en termes clairs. On examine aussi l'importance que porte l'exploration du comportement dynamique de systèmes halieutiques.},
	pages = {1366--1369},
	number = {7},
	journaltitle = {Canadian Journal of Fisheries and Aquatic Sciences},
	shortjournal = {Can. J. Fish. Aquat. Sci.},
	author = {Hilborn, Ray and Walters, Carl J.},
	urldate = {2015-08-18},
