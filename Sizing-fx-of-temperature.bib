	author = {Calduch-Verdiell, Núria and {MacKenzie}, Brian R. and Vaupel, James W. and Andersen, Ken H.},
	
	year = {2014-04-14},
	file = {NRC Research Press Snapshot:/home/philipp/biblio/zotero/storage/8JKVRW45/cjfas-2014-0034.html:text/html}
