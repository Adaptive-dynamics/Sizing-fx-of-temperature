	pmid = {17472923},
	pmcid = {PMC2442852},
	file = {PubMed Central Full Text PDF:/home/philipp/biblio/zotero/storage/4WGRHVEZ/Claireaux and Lefrançois - 2007 - Linking environmental variability and fish perform.pdf:application/pdf}
}

