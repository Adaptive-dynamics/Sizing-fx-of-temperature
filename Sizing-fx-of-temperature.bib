	year = {2016},
	langid = {english},
	keywords = {Environmental effects, intraclass correlation, personality, phenotypic plasticity, reaction norm, temperature},
	file = {Full Text PDF:/home/philipp/biblio/zotero/storage/GWF7DJE4/Killen et al. - 2016 - Context dependency of trait repeatability and its .pdf:application/pdf;Snapshot:/home/philipp/biblio/zotero/storage/GF8MUVR5/cow007.html:text/html}
}


