	keywords = {balanced harvesting, ecosystem-based fisheries management, maximum sustainable yield, selective fisheries, size spectrum},
	file = {Jacobsen et al. - 2014 - The consequences of balanced harvesting of fish co.pdf:/home/philipp/biblio/zotero/storage/THASVXE5/Jacobsen et al. - 2014 - The consequences of balanced harvesting of fish co.pdf:application/pdf;Snapshot:/home/philipp/biblio/zotero/storage/6AP82CU4/20132701.html:text/html}
}

