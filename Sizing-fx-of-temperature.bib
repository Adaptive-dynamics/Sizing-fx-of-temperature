@article{killenintraspecific2010,
	title = {The intraspecific scaling of metabolic rate with body mass in fishes depends on lifestyle and temperature},
	volume = {13},
	
	
	doi = {10.1111/j.1461-0248.2009.01415.x},
	abstract = {Ecology Letters (2010) 13: 184–193 
Abstract
